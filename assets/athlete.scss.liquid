/* ===== NAVBAR ===== */

header a { font-family:{{settings.nav_font}}; }

/* Toolbar */
.header-button {
	padding: 18px 0;
	a { letter-spacing: 2px; border-right: 1px solid #fff; color: #fff !important; font-size: 18px; line-height: 14px; padding: 0 12px;}
	&:last-child a {border: none;}
}

.heada { background: #606060; }



.shop-button-header > a {
	letter-spacing: 2px; padding: 13px 6px; color: #fff !important; min-width: 80px; border-right: 2px solid {{settings.color_two}};

	i {margin-right: 6px; font-size: 18px; line-height: 18px;}
}

.shop-button-header:last-child > a {border-left: 2px solid {{settings.color_two}};}

label {
    display:block;

    padding:0;
    top:0;
    left:0;
    cursor:pointer;
}

.searchbar {
    background-color: {{settings.color_three}};
    font-size:0;
    position:absolute;
    width:48px;
    margin-top: 8px;
    border: 0px;
}

.search-form {
	position: relative;
	float: left;
	background: transparent;
	border: 0;
	margin: 0;
	padding: 0;
	vertical-align: baseline;
	list-style: none;

	input[type="ser"] {
	    width:42px;
	    height: 42px;
	    margin-right: 6px;
	    margin-top: 8px;
	    background: {{settings.color_three}};
	    border-color: {{settings.color_three}}!important;
	    color: black;
	    padding:0;
	    padding-left:44px; /* account for 4px border to get perfect circle */
	    /*border:solid 2px rgb(200,200,200);*/

	    font-weight:300;
	    font-size:18px;

	   /* -webkit-border-radius:24px;
	       -moz-border-radius:24px;
	            border-radius:24px;*/

	    -webkit-transition:width 0.5s ease-in-out;
	       -moz-transition:width 0.5s ease-in-out;
	            transition:width 0.5s ease-in-out;

		&:focus,
		&:active {
	    	width:480px;
	    	padding-left: 49px;
	    	outline-color: {{settings.color_one}};

		   /* -webkit-box-shadow:4px 4px 8px rgb(220,220,220);
		       -moz-box-shadow:4px 4px 8px rgb(220,220,220);
		            box-shadow:4px 4px 8px rgb(220,220,220);*/
		}
	}
}

.content-right{ padding-top: 18px; }

/* Desktop Navbar */

.navbar-brand { height:auto!important; }
.navbar-toggle { margin-top: 18px!important; }
.navbar.navbar-default.navbar-fixed-top {
	border:none;
	/*box-shadow: 0px 0px 3px #9c9c9c;*/
	z-index:1000;
	background-color: {{settings.color_one}};

	.navbar-brand {
		letter-spacing:2px; color: #FFF; font-size: 44px; padding:20px!important;
		img { margin-right:20px; }
	}
	@media (max-width: 992px) and (min-width: 767px){
		.nav.navbar-nav {
			float: none;
			li a { font-size: 22px;}
		}
	}
	@media (max-width: 766px) {
		.nav.navbar-nav { text-align:center; }
	}
	@media (min-width: 323px) and (max-width: 768px){
		li a {
			padding-top: 12px;
			padding-bottom: 12px;
			margin-right: 0px;
		}
	}
}

/* Mobile Nav */

.navbar-toggle {
	border-color: #ddd;
	margin-right: 10px;
	@media (max-width: 400px) {
		margin-right: 5px!important;
	}
}

.nav.navbar-nav{

	@media (min-width: 768px){
		float: right;
	}

	@media (max-width:767px) {
		li a {
			padding-top:10px !important;
			padding-bottom:10px !important;
			text-align: center;
		}
	}

	li a {
		color:#FFF;
		font-size:24px;
		letter-spacing:2px;
		padding-top:35px;
		padding-bottom:35px;
		margin-right:8px;

		&:hover {
			background-color:transparent;
			color:{{settings.color_two}};
		}
	}

	.dropdown .dropdown-menu {
		box-shadow:none;
		background-color:#141c21;
		border:none;
		border-radius:0px;
		padding:0px;
		margin-top:0px;

		li a {
		  	font-family:{{settings.nav_font}};
			margin:0px 0px 0px 0px;
			padding:10px;
			color:#455c68;
			letter-spacing:0px;
			font-size:12px;
			border-bottom-style:solid;
			border-bottom-width:1px;
			border-bottom-color:#172126;

			&:hover { background-color:#182227; color:#7f9baa; }
		}
	}
}

