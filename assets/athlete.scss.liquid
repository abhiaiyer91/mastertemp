  transition: all .2s ease-in-out;
  display: inline-block;
  max-width: 100%;
  height: auto;
}

/* ===== BUTTONS ===== */

.button {
	margin: .4em;
  padding: 1em;
  cursor: pointer;
  text-decoration: none;
  background: {{settings.color_two}};
  font-family: {{settings.but_font}};
  color:white;
  border-style: {{settings.color_two}};
  border-width:2px;
  border-radius:4px;
  padding:12px 60px 12px 60px;
  display:inline-block;
  -webkit-transition: background 0.3s linear;
    -moz-transition: background 0.3s linear;
    -ms-transition: background 0.3s linear;
    -o-transition: background 0.3s linear;
    transition: background 0.3s linear;
}


.button:hover {
  background-color:{{settings.color_one}};
  border-color:white;
  text-decoration:none;
  color:#FFF;
}

.button.blog {
		color:#FFF;
		border-color:{{settings.color_two}};
		padding:8px 20px 8px 20px;

		&:hover {
